<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.japkaur.one.MainActivity">

    <Button
        android:id="@+id/donateabook"
        android:layout_width="195dp"
        android:layout_height="89dp"
        android:text="Donate a book"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="70dp"
        android:onClick="openCreateAvailable"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="117dp" />

    <Button
        android:id="@+id/requestabook"
        android:layout_width="198dp"
        android:layout_height="90dp"
        android:text="Request a book"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="70dp"
        android:onClick="openCreateRequest"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="269dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="90dp">

    </LinearLayout>
</LinearLayout>
